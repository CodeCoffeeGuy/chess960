{
  "name": "@chess960/db",
  "version": "0.1.0",
  "description": "Database package for Chess960 platform with Prisma ORM",
  "keywords": ["chess", "chess960", "database", "prisma", "postgresql"],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeAndCoffeeGuy/chess960.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "prisma generate && tsc",
    "dev": "tsc --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "extract-puzzles": "tsx scripts/extract-puzzles.ts",
    "generate-endgame-puzzles": "tsx scripts/generate-endgame-puzzles.ts",
    "extract-endgame-puzzles": "tsx scripts/extract-endgame-puzzles.ts",
    "generate-endgame-stockfish": "tsx scripts/generate-endgame-puzzles-stockfish-vs-stockfish.ts",
    "generate-puzzles": "tsx scripts/generate-chess960-puzzles.ts",
    "generate-practices": "tsx scripts/generate-practices.ts",
    "seed-practices-from-puzzles": "tsx scripts/seed-practices-from-puzzles.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^16.0.0",
    "@chess960/stockfish": "workspace:*",
    "@chess960/utils": "workspace:*",
    "@chess960/puzzle-generator": "workspace:*",
    "chess.js": "^1.0.0-beta.8"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "prisma": "^5.20.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0"
  }
}